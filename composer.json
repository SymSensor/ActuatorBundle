{
    "name": "symsensor/actuator-bundle",
    "description": "Extensible bundle to provide runtime health or information about a deployed application",
    "keywords": [ "actuator" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^10.3",
        "mikey179/vfsstream": "^1.6",
        "symfony/browser-kit": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SymSensor\\ActuatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymSensor\\ActuatorBundle\\Tests\\": "tests/"
        }
    }
}
